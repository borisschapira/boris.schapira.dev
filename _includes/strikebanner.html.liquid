    position: absolute;
    z-index: -1;
    top: 1px;
    left: 1px;
    right: 1px;
    bottom: 1px;
    border: 10px solid yellow
  }

  .strike512-textcontainer {
    flex: 1;
    text-align: center
  }

  .strike512-line {
    display: block
  }

  .strike512-line1 {
    margin-top: 1rem;
    font-size: calc(1em + 1vw)
