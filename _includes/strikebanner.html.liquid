    }
  }

  .strike512-line3 {
    font-size: calc(1em + 1vw);
    font-family: "Homemade Apple", cursive;
    color: yellow;
    transform: rotate(-2deg)
  }

  .strike512-fistscontainer {
    flex: 1;
    display: none;
    grid-row-gap: 5px
  }

  @media (min-width: 45rem) {
    .strike512-fistscontainer {
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      grid-column-gap: 5px
    }
  }

  @media (min-width: 65rem) {
    .strike512-fistscontainer {
      display: grid;
      grid-template-columns: repeat(4, 1fr);
      grid-column-gap: 5px
    }
  }

  .strike512-fist {
    margin: auto;
    height: 10vw;
    width: 10vw
  }

  .strike512-fist1 {
    color: green
  }

  .strike512-fist2 {
    color: yellow
  }

  .strike512-fist3 {
    color: purple
