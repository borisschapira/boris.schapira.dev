{
  "name": "jekyll-private",
  "version": "1.0.0",
  "description": "My Jekyll v3 setup.",
  "scripts": {
    "pretty:css": "prettier --write '_src/styles/scss/**/*.scss'",
    "pretty:js": "prettier --write '_src/scripts/custom/**/*.js'",
    "pretty": "npm run pretty:js && npm run pretty:css",
    "pretty:md": "prettier --write '{,!(vendor)/**/*.md}'",
    "pretty:md:2019": "prettier --write '{,!(vendor)/**/2019/*.md}'",
    "pretty:md:bookmark": "prettier --write '_bookmarks/**/*.md' > /dev/null",
    "build": "webpack",
    "postbuild": "node ./scripts/compute-hashes.js",
    "lambda:serve": "netlify-lambda serve functions",
    "lambda:build": "netlify-lambda build functions",
    "comments:get": "node ./scripts/get-comments.js",
    "bookmarks:get": "node ./scripts/get-bookmarks.js && npm run pretty:md:bookmark",
    "test:visual": "mocha --timeout 15000 scripts/visual-regression.js",
    "watch": "npm-scripts-watcher"
  },
  "watch": {
    "_src/styles/scss/**/*.scss": [
      "pretty:css",
      "build:css"
    ],
    "_src/scripts/**/*.js": [
      "pretty:js",
      "build:js"
    ],
    "_posts/**/2019/*.md": [
      "pretty:md:2019"
    ]
  },
  "config": {
    "css_dir": "_src/styles/css/",
    "scss_dir": "_src/styles/scss/",
    "dist_dir": "assets/styles/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borisschapira/jekyll-bs.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/borisschapira/jekyll-bs/issues"
  },
  "homepage": "https://github.com/borisschapira/jekyll-bs#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "abbr-touch": "^2.1.6",
    "algoliasearch": "^4.0.2",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "concat-cli": "^4.0.0",
    "csswring": "^7.0.0",
    "easy-toggle-state": "^1.12.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-import": "^2.20.1",
    "first-input-delay": "^0.1.3",
    "include-media": "^1.4.9",
    "install": "^0.13.0",
    "instantsearch.js": "^4.2.0",
    "jquery": "^3.4",
    "node-sass": "^4.13.1",
    "normalize-scss": "^7.0.1",
    "npm-scripts-watcher": "^1.0.2",
    "pixelmatch": "^5.1.0",
    "pngjs": "^3.4.0",
    "postcss": "^7.0.26",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "purify-css": "^1.2.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "touchtap-event": "^2.0.2",
    "uglify-es": "github:mishoo/UglifyJS2#harmony",
    "var": "^0.4.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.10"
  },
  "optionalDependencies": {
    "mocha": "^7.0.1",
    "chai": "^4.2.0",
    "puppeteer": "^2.1.1"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "escape-string-regexp": "^2.0.0",
    "file-loader": "^5.0.2",
    "hasha": "^5.1.0",
    "js-md5": "^0.7.3",
    "js-yaml": "^3.12.0",
    "mkdirp": "^1.0.3",
    "netlify-lambda": "^1.6.3",
    "npm": "^6.13.7",
    "rss-parser": "^3.7.5",
    "slugify": "^1.3.6"
  }
}
