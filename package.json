{
  "name": "boris-schapira-dev-jekyll",
  "version": "1.0.0",
  "description": "My Jekyll setup.",
  "scripts": {
    "pretty:css": "prettier --write '_src/styles/scss/**/*.scss'",
    "pretty:md": "prettier --write '{,!(vendor)/**/*.md}'",
    "pretty:js": "prettier --write '_src/scripts/**/*.js'",
    "pretty": "npm-run-all 'pretty:*'",
    "build": "webpack",
    "postbuild": "node ./scripts/compute-hashes.js",
    "lambda:serve": "netlify-lambda serve functions",
    "lambda:build": "netlify-lambda build functions",
    "get-comments": "node ./scripts/get-comments.js",
    "test:feeds": "node ./scripts/validate-feeds.js",
    "test:visual": "mocha --timeout 15000 scripts/visual-regression.js",
    "watch": "npm-scripts-watcher"
  },
  "watch": {
    "_src/styles/scss/**/*.scss": [
      "pretty:css",
      "build"
    ],
    "_src/scripts/**/*.js": [
      "pretty:js",
      "build"
    ],
    "_posts/**/2019/*.md": [
      "prettier --write '{,!(vendor)/**/2019/*.md}'"
    ]
  },
  "config": {
    "css_dir": "_src/styles/css/",
    "scss_dir": "_src/styles/scss/",
    "dist_dir": "assets/styles/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borisschapira/jekyll-bs.git"
  },
  "author": "borisschapira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/borisschapira/jekyll-bs/issues"
  },
  "homepage": "https://github.com/borisschapira/jekyll-bs#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "abbr-touch": "^2.1.6",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "blurhash": "^1.1.3",
    "canvas": "^2.6.1",
    "concat-cli": "^4.0.0",
    "csswring": "^7.0.0",
    "easy-toggle-state": "^1.14.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-import": "^2.22.1",
    "fast-xml-parser": "^3.17.4",
    "first-input-delay": "^0.1.3",
    "globby": "^11.0.1",
    "hashmap": "^2.4.0",
    "include-media": "^1.4.9",
    "node-sass": "^4.14.1",
    "normalize-scss": "^7.0.1",
    "npm": "^6.14.8",
    "npm-run-all": "^4.1.5",
    "npm-scripts-watcher": "^1.0.2",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.1.0",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "purify-css": "^1.2.5",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "webpack": "^5.3.0",
    "webpack-cli": "^4.1.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "escape-string-regexp": "^4.0.0",
    "file-loader": "^6.2.0",
    "hasha": "^5.2.2",
    "instant.page": "^5.1.0",
    "js-md5": "^0.7.3",
    "js-yaml": "^3.14.0",
    "mkdirp": "^1.0.4",
    "mocha": "^8.2.0",
    "netlify-build-plugin-dareboost": "^1.2.1",
    "netlify-lambda": "^2.0.1",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.9.0",
    "slugify": "^1.4.6",
    "touchtap-event": "^2.0.2",
    "turbolinks": "^5.2.0",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-sw": "^5.1.4"
  }
}
